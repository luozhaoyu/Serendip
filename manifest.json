{
    "name": "Serendip",
    "version": "0.12",
    "manifest_version": 2,
    "description": "Save the biggest visible picture on your screen",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "tabs",
        "downloads",
        "<all_urls>"
        ],
    "content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": [
            "content.js"
            ]
    }
    ]
}
